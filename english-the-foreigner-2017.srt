- <b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">SLAMMING DOOR</font> <font color="#9b75ff">)</font></b>
- <b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">BREATHING HEAVILY</font> <font color="#9b75ff">)</font></b>

502
00:39:23,100 --> 00:39:24,230
<b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">GLASS BREAKS</font> <font color="#9b75ff">)</font></b>

503
00:39:31,480 --> 00:39:33,400
<b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">BOTH GRUNTING</font> <font color="#9b75ff">)</font></b>

