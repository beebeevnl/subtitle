<b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">GROANS</font> <font color="#9b75ff">)</font></b>

1225
01:36:02,500 --> 01:36:03,660
<b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">GROANS</font> <font color="#9b75ff">)</font></b>

1226
01:36:05,960 --> 01:36:07,590
<b><font color="#9b75ff">(</font> <font face="Mistral" color="#80ff00">WHIMPERING</font> <font color="#9b75ff">)</font></b>

1227
